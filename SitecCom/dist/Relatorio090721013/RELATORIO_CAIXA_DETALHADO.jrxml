	<queryString>
		<![CDATA[select caixa.*,coalesce(caixa.pesoaorcamento, 0) as porc ,coalesce(caixa.receber, 0) as pdup,DATE_FORMAT(DATAPAGAMENTO, '%d.%m.%Y') AS dpagamento,pessoa.nome as nome,empresa.nome as nomeempresa
,empresa.cnpj as cnpjempresa,empresa.endereco as enderecoempresa,empresa.numero as numeroempresa
,empresa.bairro as bairroempresa,empresa.cidade as cidadeempresa,empresa.telefone as telefoneempresa
,empresa.cep as cepempresa from caixa
left join pessoa ON CAIXA.PESSOA=pessoa.id
left join empresa on caixa.empresa=empresa.id
where CAIXA.EMPRESA=$P{EMPRESA} AND DATAPAGAMENTO BETWEEN $P{DATAINICIAL} AND $P{DATAFINAL} order by caixa.formapg]]>
	</queryString>
