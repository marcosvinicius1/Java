	<queryString>
		<![CDATA[select pesoaorcamento.id,pesoaorcamento.data,pesoaorcamento.vorcamento,pessoa.cpf_cnpj
,pessoa.inscricao,pessoa.nome as cliente,pessoa.telefone,pessoa.numero,rua.nome as rua,rua.bairro,rua.cep
,p.nome as vendedor,orcamento.desconto,orcamento.formapg,orcamento.precotproduto
,orcamento.precovenda,orcamento.qtde,produto.codbarra,produto.descricao,empresa.nome as empresa
,empresa.cnpj as cnpjempresa,empresa.endereco as enderecoempresa,empresa.numero as numeroempresa
,empresa.bairro as bairroempresa,empresa.cidade as cidadeempresa,empresa.telefone as telefoneempresa
,empresa.cep as cepempresa
from pesoaorcamento
LEFT JOIN orcamento ON orcamento.pessoa = pesoaorcamento.id
LEFT JOIN pessoa ON pesoaorcamento.pessoacliente = pessoa.id
LEFT JOIN pessoa p on pesoaorcamento.pessoavendedor = p.id
left JOIN rua ON pessoa.rua = rua.id
left JOIN produto ON orcamento.produto = produto.id
left join empresa on pesoaorcamento.empresa=empresa.id where pesoaorcamento.id =$P{numero}]]>
	</queryString>
