	<queryString>
		<![CDATA[select receber.*,coalesce(receber.norcamento,0)as norc,coalesce(receber.ndocumento,0) as ndoc,pessoa.nome as nomecliente,pessoa.cpf_cnpj as cnpjcliente,p.nome as nomevendedor,empresa.nome as nomeempresa
,empresa.cnpj as cnpjempresa,empresa.endereco as enderecoempresa,empresa.numero as numeroempresa
,empresa.bairro as bairroempresa,empresa.cidade as cidadeempresa,empresa.telefone as telefoneempresa
,empresa.cep as cepempresa from receber
LEFT JOIN pessoa on receber.cliente=pessoa.id
LEFT JOIN pessoa p on receber.vendedor=p.id
inner join empresa on empresa.id=receber.empresa
where $P!{campo} and receber.empresa=$P{empresa} and receber.pg<>$P{pg}]]>
	</queryString>
